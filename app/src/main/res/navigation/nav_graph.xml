<?xml version="1.0" encoding="utf-8"?>
<navigation xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/main_navigation"
    app:startDestination="@id/fragmentOne">
    <fragment
        android:id="@+id/fragmentOne"
        android:name="com.hyperelement.mvvmdemo.ui.fragment.list.FragmentMovieList"
        android:label="FragmentOne"
        tools:layout="@layout/fragment_list">

        <action
            android:id="@+id/action_fragmentOne_to_fragmentDetail"
            app:destination="@id/fragmentDetail" />

    </fragment>
    <fragment
        android:id="@+id/fragmentDetail"
        android:name="com.hyperelement.mvvmdemo.ui.fragment.detail.FragmentDetail"
        android:label="FragmentDetail"
        tools:layout="@layout/fragment_detail">
        <argument
            android:name="movie"
            app:argType="com.hyperelement.mvvmdemo.data.entity.search.Results" />
    </fragment>

</navigation>